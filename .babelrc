{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "react-hot-loader/babel",
    ["fast-async", {
      "env": {
        "augmentObject": false,
        "dontMapStackTraces": true,
        "asyncStackTrace": false,
        "dontInstallRequireHook": false
      },
      "compiler": {
        "promises": true,
        "generators": false
      },
      "runtimePattern": null
    }]
  ]
}